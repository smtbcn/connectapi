# Veritabanı Bağlantı Bilgileri
# Bu dosyayı .env olarak kopyalayın ve gerçek değerlerle doldurun

DB_HOST=your_database_host
DB_USERNAME=your_database_username
DB_PASSWORD=your_database_password
DB_NAME=your_database_name
DB_CHARSET=utf8

# API Konfigürasyonu
API_BASE_URL=https://your-api-domain.com
API_CLIENT_ID=your_client_id
API_CLIENT_SECRET=your_client_secret
API_APP_CODE=your_app_code

# Bayi Bilgileri (Format: username|password|orderer|name)
BAYI_0=username1|password1|orderer1|Bayi Adı 1
BAYI_1=username2|password2|orderer2|Bayi Adı 2
BAYI_2=username3|password3|orderer3|Bayi Adı 3
# BAYI_3=username4|password4|orderer4|Bayi Adı 4

# Aktif bayi sayısı
AKTIF_BAYI_SAYISI=3

# Rapor domain'i
REPORT_DOMAIN=your-report-domain.com

# Config versiyonu
CONFIG_VERSION=v1.1